{
    "name": "c975l/payment-bundle",
    "type": "symfony-bundle",
    "description": "Defines form, transactions, database store, etc. for payments using Stripe",
    "keywords": ["symfony", "bundle", "stripe", "payment"],
    "homepage": "https://github.com/975L/PaymentBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/LaurentMarquet",
            "role": "Developer"
        },
        {
            "name": "975L Community",
            "homepage": "https://github.com/975L"
        }
    ],
    "require": {
        "php": "^7",
        "c975l/config-bundle": "^2",
        "c975l/email-bundle": "^3",
        "c975l/includelibrary-bundle": "^1",
        "c975l/services-bundle": "^1",
        "c975l/toolbar-bundle": "^1",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/orm": "^2",
        "stripe/stripe-php": "^7",
        "symfony/form": "*",
        "symfony/security": "*",
        "symfony/translation": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0",
        "phpunit/phpunit": "^7",
        "rector/rector": "^0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "c975L\\PaymentBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "email": "contact@975l.com",
        "source": "https://github.com/975L/PaymentBundle",
        "issues": "https://github.com/975L/PaymentBundle/issues"
    }
}
